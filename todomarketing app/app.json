{
  "expo": {
    "name": "Todo Marketing",
    "slug": "newtodomarketing",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": [
        "WRITE_EXTERNAL_STORAGE"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.codezeal.newtodomarketing",
      "navigationBar": {
        "icon": "./assets/icon.png"
      },
      "config": {
        "googleMaps": {
          "apiKey": ""
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": ""
      }
    },
    "owner": "codezeal",
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "La aplicaci√≥n necesita acceder a tus fotos/videos para funcionar correctamente."
        }
      ],
      "expo-router"
    ]
  }
}
